{
	"require": {
		"laravel/framework": "dev-master",
		"tcpdf/tcpdf": "dev-master",
		"filp/whoops": "dev-master",
		"endroid/qrcode": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "dev-master"
	},
	"autoload": {
		"classmap": [
            "app/exceptions",
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database",
            "app/tests"
        ],
        "files": [
        	"app/helpers.php"
        ]
	},
	"scripts": {
		"post-update-cmd": "php artisan optimize"
	},
	"config": {
		"process-timeout": 600
	},
	"minimum-stability": "dev"
}